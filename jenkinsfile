pipeline {
    agent any

    tools {
       maven 'Maven 3.0.5'
    }
    stages {
        stage('Maven Version') {
            steps {
				sh 'mvn --version'
            }
            }
        stage('Host Name') {
            steps {
				sh 'hostname'
            }
            }
        stage('Git Clone') {
            steps {
				git credentialsId: 'github1', url: 'https://github.com/nsikbal/nsikbal.git'
            }
            }
        stage('Sonar Scan') {
            steps {
				 sh 'mvn sonar:sonar \
  -Dsonar.host.url=http://44.192.44.234:9000 \
  -Dsonar.login=81e152d406dd47505c04ec29c1215008a7ef6b26'
			}
            }
		stage('Clean') {
            steps {
				  sh 'mvn clean' 
				}
            }
        stage('Validate') {
            steps {
				 sh 'mvn validate' 
			}
            }
        stage('Compile') {
            steps {
				 sh 'mvn compile' 
            }
            }
        stage('Test') {
            steps {
				 sh 'mvn test' 
            }
            }
        stage('Package') {
            steps {
				 sh 'mvn package' 
			
            }
            }
        }
}

